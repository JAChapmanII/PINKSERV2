
template<typename T> bool
		util::contains(std::vector<T> vec, T val) { // {{{
	return (find(vec.begin(), vec.end(), val) != vec.end());
} // }}}
template<typename K, typename V> bool
		util::contains(std::map<K, V> map, K key) { // {{{
	return (map.find(key) != map.end());
} // }}}

